	Journal = {Journal of the Mechanics and Physics of Solids},
	Keywords = {homogenization, damage, finite element, failure},
	Number = {7},
	Pages = {1017--1044},
	Publisher = {Elsevier},
	Title = {Homogenization-based continuum plasticity-damage model for ductile failure of materials containing heterogeneities},
	Volume = {57},
	Year = {2009},
