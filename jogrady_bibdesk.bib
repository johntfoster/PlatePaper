@article{drugan1996micromechanics,
	Abstract = {A variational formulation is employed to derive a micromechanics-based, explicit nonlocal constitutive equation relating the ensemble averages of stress and strain for a class of random linear elastic composite materials. For two-phase composites with any isotropic and statistically uniform distribution of phases (which themselves may have arbitrary shape and anisotropy), we show that the leading-order correction to a macroscopically homogeneous constitutive equation involves a term proportional to the second gradient of the ensemble average of strain. This nonlocal constitutive equation is derived in explicit closed form for isotropic material in the one case in which there exists a well-founded physical and mathematical basis for describing the material's statistics: a matrix reinforced (or weakened) by a random dispersion of nonoverlapping identical spheres. By assessing, when the applied loading is spatially-varying, the magnitude of the nonlocal term in this constitutive equation compared to the portion of the equation that relates ensemble average stresses and strains through a constant ``overall'' modulus tensor, we derive quantitative estimates for the minimum representative volume element (RVE) size, defined here as that over which the usual macroscopically homogeneous ``effective modulus'' constitutive models for composites can be expected to apply. Remarkably, for a maximum error of 5% of the constant ``overall'' modulus term, we show that the minimum RVE size is at most twice the reinforcement diameter for any reinforcement concentration level, for several sets of matrix and reinforcement moduli characterizing large classes of important structural materials. Such estimates seem essential for determining the minimum structural component size that can be treated by macroscopically homogeneous composite material constitutive representations, and also for the development of a fundamentally-based macroscopic fracture mechanics theory for composites. Finally, we relate our nonlocal constitutive equation explicitly to the ensemble average strain energy, and show how it is consistent with the stationary energy principle.},
	Annote = {Micromechanics model for linear elastic composites
isotropic and statistically uniform distribution of phases
use second gradient of "ensemble average of strain"
create minimum representative volume element size

creates a rule to homogenize material based on RVE
},
	Author = {Drugan, WJ and Willis, JR},
	Date-Added = {2012-01-13 21:50:00 +0000},
	Date-Modified = {2012-04-02 15:35:39 +0000},
	Doi = {10.1016/0022-5096(96)00007-5},
	Journal = {Journal of the Mechanics and Physics of Solids},
	Keywords = {composites, RVE, homogenization, constitutive},
	Number = {4},
	Pages = {497--524},
	Publisher = {Elsevier},
	Title = {A micromechanics-based nonlocal constitutive equation and estimates of representative volume element size for elastic composites},
	Volume = {44},
	Year = {1996},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://dx.doi.org/10.1016/0022-5096(96)00007-5}}

@article{choi1992model,
	Abstract = {An investigation was performed to study the impact damage of graphite/epoxy laminated composites caused by a low-velocity foreign object. The impact
damage in terms of matrix cracking and delaminations resulting from a point-nose impac- tor was the primary concern. A model was developed for predicting the initiation of the damage and the extent of the final damage as a function of material properties, laminate configuration and the impactor's mass. The model consists of a stress analysis and a failure analysis. A transient dynamic finite element analysis was adopted for calculating the stresses and strains inside the composites during impact resulting from a point-nose im- pactor. Failure criteria were proposed for predicting the initial intraply matrix cracking andthesizeoftheinterfacedelaminationsinthecomposites.Experimentswere alsoper- formed to verify the model and the computer simulations. The predictions agreed fairly well with the test data.},
	Annote = {There is a vast quantity of impact data for composite materials, but analytical studies primarily focus on the contact force of the impactor or the plate response regardless of damage. There are no analytical models that can accurately predict impact damage in laminated composites.
This study takes impact data with a variety of impactor noses to control the type of loading. They used line-nose impactors to reduce the problem from 3D to 2D.
Impactor force was calculated from Hertzian theory.
Through-thickness finite elements were used to model the plate, including different ply orientations within a single element.
Various damage mechanisms were observed:
1) Primary damage was intraply matrix cracks (shear/bending matrix cracks)
2) Delamination initiate from 1) and propagate into material interface
3) Extensive multiple micro-matrix cracks along with 2)
4) Shear matrix crack in inner plies generates substantial delamination in bottom interface and confined delamination in upper interface of cracked ply
5) Bending matrix crackat surface of lamines generates delamination along first interface of cracked ply
Findings believed to be applicable to point impact as well
Matrix failure was predicted by Choi's criterion (s_yy/Y)^2 + (s_yx/S)^2=e^2 Y -s tensile or compressive strength, S is interlaminar shear strength. S is poorly characterized and it and Y were computed from the change in angle between plies and 3 empirical parameters.
While crack prediction is analytical, delamination prediction is semi-empirical
Delamination caused when 1) ply above or below fails in matrix cracking, THEN 2)combined stresses governing delamination growth through thickness of upper and lower ply groups reaches a critical value.
Crit value ed^2=D([s^n_yz/S^n]^2+[s^n+1_xz/S^n+1]^2+[s^n+1_yy/Y^n+1]^2)
D is empirical constant material property determined from experiments, insensitive to ply orientation or laminate thickness. (n=upper ply, n+1=lower ply)
Predicted delaminations were seen, but there were also unpredicted delaminations-induced by a shear matrix crack
Overall, predictions matched experiments well, but there were some delaminations for which threshold impactor velocity was significantly underestimated.
Findings:
l.There exists an impact velocity threshold for laminated composites below which no delamination occurs but above which significant damage is pro- duced.
2. Matrix cracking is the initial failure mode.
3. Matrix cracking in composites can lead to interface delaminations.
4. The occurrence of the critical matrix cracks is primarily due to the inter-
laminar transverse shear stress and transverse in-plane stress.
5. Delamination growth is governed by the interlaminar longitudinal shear stress and transverse in-plane stress in the layer below the delaminated interface and the interlaminar transverse shear stress in the upper layers right above the
},
	Author = {Choi, H.Y. and Chang, F.K.},
	Date-Added = {2012-01-13 20:34:08 +0000},
	Date-Modified = {2012-06-19 18:37:31 +0000},
	Doi = {10.1177/002199839202601408},
	Journal = {Journal of Composite Materials},
	Keywords = {composites, damage, experimental, finite element},
	Number = {14},
	Pages = {2134--2169},
	Publisher = {SAGE Publications},
	Title = {A model for predicting damage in graphite/epoxy laminated composites resulting from low-velocity point impact},
	Volume = {26},
	Year = {1992},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://dx.doi.org/10.1177/002199839202601408}}

